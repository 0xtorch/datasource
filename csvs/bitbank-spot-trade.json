{
  "id": "bitbank-spot-trade",
  "service": "bitbank",
  "formatter": {
    "取引id": ["string"],
    "通貨ペア": ["string"],
    "売/買": ["string"],
    "数量": ["big-decimal-positive"],
    "価格": ["big-decimal-positive"],
    "手数料": ["big-decimal"],
    "取引日時": ["iso8601-jp"]
  },
  "parser": {
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "売/買"
            },
            "pattern": {
              "type": "in",
              "values": ["buy", "sell"]
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot-trade_",
                {
                  "type": "cell",
                  "column": "取引id"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引日時"
            },
            "transfers": [
              {
                "target": [
                  {
                    "column": "売/買",
                    "pattern": "buy"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "_",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "売/買",
                    "pattern": "sell"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "_",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "売/買",
                    "pattern": "buy"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "math",
                    "values": [
                      {
                        "type": "cell",
                        "column": "数量"
                      },
                      "*",
                      {
                        "type": "cell",
                        "column": "価格"
                      }
                    ]
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "_",
                    "index": 1
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "売/買",
                    "pattern": "sell"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "math",
                    "values": [
                      {
                        "type": "cell",
                        "column": "数量"
                      },
                      "*",
                      {
                        "type": "cell",
                        "column": "価格"
                      }
                    ]
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "_",
                    "index": 1
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "手数料",
                    "pattern": {
                      "type": "positive"
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "_",
                    "index": 1
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "手数料",
                    "pattern": {
                      "type": "negative"
                    }
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "_",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "BCC": "crypto/bitcoin-cash",
    "KLAY": "crypto/klay-token",
    "MONA": "crypto/monacoin",
    "RNDR": "crypto/render-token"
  }
}
