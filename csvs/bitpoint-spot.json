{
  "id": "bitpoint-spot",
  "service": "bitpoint",
  "formatter": {
    "No": ["big-decimal"],
    "受渡日": ["blank", "datetime-jp"],
    "約定日時": ["blank", "datetime-jp"],
    "取引種類": ["string"],
    "通貨コード１": ["blank", "string"],
    "通貨コード２": ["blank", "string"],
    "売買": ["blank", "string"],
    "参考単価": ["blank", "big-decimal-positive"],
    "数量": ["blank", "big-decimal-positive"],
    "約定番号": ["blank", "big-decimal-positive"],
    "円": ["blank", "big-decimal"],
    "手数料": ["blank", "big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "現物取引 - 買",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種類"
            },
            "pattern": "現物取引"
          },
          {
            "value": {
              "type": "cell",
              "column": "売買"
            },
            "pattern": "買"
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "約定日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "約定日時"
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "通貨コード１"
                  }
                }
              },
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "円"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "通貨コード２"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "現物取引 - 売",
