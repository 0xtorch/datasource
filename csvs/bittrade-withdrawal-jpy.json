{
  "id": "bittrade-withdrawal-jpy",
  "service": "bittrade",
  "formatter": {
    "申請日時": ["datetime-jp"],
    "処理時間": ["datetime-jp"],
    "注文ID": ["string"],
    "銘柄": ["string"],
    "金額": ["big-decimal"],
    "手数料": ["big-decimal"],
    "合計金額": ["big-decimal"],
    "出金先口座": ["string"],
    "ステータス": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "完了 with fee",
        "conditions": [
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": "完了"
          },
          {
            "value": { "type": "cell", "column": "手数料" },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal-jpy_",
                { "type": "cell", "column": "注文ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "金額" },
                "JPY to ",
                { "type": "cell", "column": "出金先口座" },
                " | ステータス: ",
                { "type": "cell", "column": "ステータス" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": { "type": "cell", "column": "手数料" }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "銘柄"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "完了 without fee",
        "conditions": [
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": "完了"
          },
          {
            "value": { "type": "cell", "column": "手数料" },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal-jpy_",
                { "type": "cell", "column": "注文ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "金額" },
                "JPY to ",
                { "type": "cell", "column": "出金先口座" },
                " | ステータス: ",
                { "type": "cell", "column": "ステータス" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "not 完了",
        "conditions": [
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": {
              "type": "not-in",
              "values": ["完了"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal-jpy_",
                { "type": "cell", "column": "注文ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "金額" },
                "JPY to ",
                { "type": "cell", "column": "出金先口座" },
                " | ステータス: ",
                { "type": "cell", "column": "ステータス" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  }
}
