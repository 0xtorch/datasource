{
  "id": "bittrade-withdrawal",
  "service": "bittrade",
  "formatter": {
    "申請日時": ["iso8601-jp"],
    "処理時間": ["iso8601-jp"],
    "銘柄": ["string"],
    "種類": ["string"],
    "アドレス": ["string"],
    "Tag": ["string"],
    "数量": ["big-decimal"],
    "手数料": ["big-decimal"],
    "合計金額": ["big-decimal"],
    "ステータス": ["string"],
    "入出金ID": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "通常出金 完了 with fee",
        "conditions": [
          {
            "value": { "type": "cell", "column": "種類" },
            "pattern": "通常出金"
          },
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": "完了"
          },
          {
            "value": { "type": "cell", "column": "手数料" },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "入出金ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "種類" },
                ": ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "銘柄" },
                " to ",
                { "type": "cell", "column": "アドレス" },
                " | ステータス: ",
                { "type": "cell", "column": "ステータス" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": { "type": "cell", "column": "手数料" }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "銘柄"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "通常出金 完了 without fee",
        "conditions": [
          {
            "value": { "type": "cell", "column": "種類" },
            "pattern": "通常出金"
          },
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": "完了"
          },
          {
            "value": { "type": "cell", "column": "手数料" },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "入出金ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "種類" },
                ": ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "銘柄" },
                " to ",
                { "type": "cell", "column": "アドレス" },
                " | ステータス: ",
                { "type": "cell", "column": "ステータス" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "通常出金 not 完了",
        "conditions": [
          {
            "value": { "type": "cell", "column": "種類" },
            "pattern": "通常出金"
          },
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": {
              "type": "not-in",
              "values": ["完了"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "入出金ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "種類" },
                ": ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "銘柄" },
                " to ",
                { "type": "cell", "column": "アドレス" },
                " | ステータス: ",
                { "type": "cell", "column": "ステータス" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "Not 通常出金",
        "conditions": [
          {
            "value": { "type": "cell", "column": "種類" },
            "pattern": {
              "type": "not-in",
              "values": ["通常出金"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "入出金ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "種類" },
                ": ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "銘柄" },
                " to ",
                { "type": "cell", "column": "アドレス" },
                " | ステータス: ",
                { "type": "cell", "column": "ステータス" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "BTC": "crypto/bitcoin",
    "ETH": "crypto/ethereum",
