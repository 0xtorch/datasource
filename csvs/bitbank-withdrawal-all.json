{
  "id": "bitbank-withdrawal-all",
  "service": "bitbank",
  "formatter": {
    "コイン": ["string"],
    "日時": ["datetime-jp"],
    "数量": ["big-decimal"],
    "手数料": ["big-decimal"],
    "ラベル": ["string"],
    "ネットワーク": ["string"],
    "アドレス": ["string"],
    "Txid": ["string"],
    "ステータス": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "fee",
        "conditions": [
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": "DONE"
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                { "type": "cell", "column": "日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                { "type": "cell", "column": "アドレス" },
                " on ",
                { "type": "cell", "column": "ネットワーク" },
                " Network | TXID: ",
                { "type": "cell", "column": "Txid" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": { "type": "cell", "column": "手数料" }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "コイン"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Not DONE withdrawal",
        "conditions": [
          {
            "value": { "type": "cell", "column": "ステータス" },
            "pattern": {
              "type": "not-in",
              "values": ["DONE"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                { "type": "cell", "column": "日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "ステータス: ",
                { "type": "cell", "column": "ステータス" },
                " / ",
                { "type": "cell", "column": "数量" },
                " ",
                { "type": "cell", "column": "コイン" },
                " to ",
                { "type": "cell", "column": "アドレス" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "BCC": "crypto/bitcoin-cash",
    "KLAY": "crypto/klay-token",
    "MASK": "crypto/mask-network",
    "MONA": "crypto/monacoin",
    "RNDR": "crypto/render-token"
  }
}
