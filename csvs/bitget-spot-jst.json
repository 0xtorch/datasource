      {
        "description": "Ignore (Buy, Sell, Financial, Transfer in, Transfer out, 365, Interest, Redeem, Redemption, Subscribe, lock-up)",
        "conditions": [
          {
            "value": { "type": "cell", "column": "type" },
            "pattern": {
              "type": "in",
              "values": [
