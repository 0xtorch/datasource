{
  "id": "bitget-spot-jst",
  "service": "bitget",
  "formatter": {
    "order": ["string"],
    "date": ["iso8601-jp"],
    "coin": ["string"],
    "type": ["string"],
    "amount": ["big-decimal"],
    "fee": ["big-decimal"],
    "available": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "Airdrop Reward-A, Reward-B => airdrop",
        "conditions": [
          {
            "value": { "type": "cell", "column": "type" },
            "pattern": {
              "type": "in",
              "values": ["Airdrop Reward-A", "Airdrop Reward-B"]
            }
          }
        ],
        "generators": [
          {
            "type": "airdrop",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot_",
                { "type": "cell", "column": "order" }
              ]
            },
            "timestamp": { "type": "cell", "column": "date" },
            "comment": {
              "type": "join",
              "values": [
                "type: ",
                { "type": "cell", "column": "type" },
                " / coin: ",
                { "type": "cell", "column": "coin" },
                " / amount: ",
                { "type": "cell", "column": "amount" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": { "type": "cell", "column": "amount" }
                },
                "asset": {
                  "type": "symbol",
                  "value": { "type": "cell", "column": "coin" }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Crypto Voucher Distribution",
        "conditions": [
          {
            "value": { "type": "cell", "column": "type" },
            "pattern": "Crypto Voucher Distribution"
          }
        ],
        "generators": [
          {
            "type": "gift",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot_",
                { "type": "cell", "column": "order" }
              ]
            },
            "timestamp": { "type": "cell", "column": "date" },
            "comment": {
              "type": "join",
              "values": [
                "type: ",
                { "type": "cell", "column": "type" },
                " / coin: ",
                { "type": "cell", "column": "coin" },
                " / amount: ",
                { "type": "cell", "column": "amount" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": { "type": "cell", "column": "amount" }
                },
                "asset": {
                  "type": "symbol",
                  "value": { "type": "cell", "column": "coin" }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Deposit / Ordinary Withdrawal => fee (if fee<0)",
        "conditions": [
          {
            "value": { "type": "cell", "column": "type" },
            "pattern": {
              "type": "in",
              "values": ["Deposit", "Ordinary Withdrawal"]
            }
          },
          {
            "value": { "type": "cell", "column": "fee" },
            "pattern": { "type": "negative" }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot_",
                { "type": "cell", "column": "order" }
              ]
            },
            "timestamp": { "type": "cell", "column": "date" },
            "comment": {
              "type": "join",
              "values": [
                "type: ",
                { "type": "cell", "column": "type" },
                " / coin: ",
                { "type": "cell", "column": "coin" },
                " / amount: ",
                { "type": "cell", "column": "amount" },
                " / fee: ",
                { "type": "cell", "column": "fee" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": { "type": "cell", "column": "fee" }
                },
                "asset": {
                  "type": "symbol",
                  "value": { "type": "cell", "column": "coin" }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Deposit / Ordinary Withdrawal => ignore (if fee=0)",
        "conditions": [
          {
            "value": { "type": "cell", "column": "type" },
            "pattern": {
              "type": "in",
              "values": ["Deposit", "Ordinary Withdrawal"]
            }
          },
          {
            "value": { "type": "cell", "column": "fee" },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot_",
                { "type": "cell", "column": "order" }
              ]
            },
            "timestamp": { "type": "cell", "column": "date" },
            "comment": {
              "type": "join",
              "values": [
                "type: ",
                { "type": "cell", "column": "type" },
                " / coin: ",
                { "type": "cell", "column": "coin" },
                " / amount: ",
                { "type": "cell", "column": "amount" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "LaunchX income (451) / Exchange income / Gains / Increase exchange rate",
        "conditions": [
          {
            "value": { "type": "cell", "column": "type" },
            "pattern": {
              "type": "in",
              "values": [
                "LaunchX income",
                "451",
                "Exchange income",
                "Gains",
                "Increase exchange rate"
              ]
            }
          }
        ],
        "generators": [
          {
            "type": "gift",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
