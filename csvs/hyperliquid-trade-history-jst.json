{
  "id": "hyperliquid-trade-history-jst",
  "service": "hyperliquid",
  "formatter": {
    "time": ["datetime-jp"],
    "coin": ["string"],
    "dir": ["string"],
    "px": ["big-decimal"],
    "sz": ["big-decimal"],
    "ntl": ["big-decimal"],
    "fee": ["big-decimal"],
    "closedPnl": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "fee | Open Long / Open Short",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "dir"
            },
            "pattern": {
              "type": "in",
              "values": ["Open Long", "Open Short"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "fee"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_trade_",
                { "type": "cell", "column": "time" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Direction: ",
                { "type": "cell", "column": "dir" },
                " / Size: ",
                { "type": "cell", "column": "sz" },
                { "type": "cell", "column": "coin" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": "USDC"
                }
              }
            ]
          }
        ]
      },
      {
        "description": "realize-profit | Close Long / Close Short (closedPnl>0)",
