                    "type": "cell",
                    "column": "約定金額"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "ペア"
                    },
                    "splitter": "_",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "BUY"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "合計"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "ペア"
                    },
                    "splitter": "_",
                    "index": 1
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "SELL"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "合計"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "ペア"
                    },
                    "splitter": "_",
                    "index": 1
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "手数料",
                    "pattern": {
                      "type": "positive"
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
