{
  "id": "mexc-spot-trade",
  "service": "mexc",
  "formatter": {
    "ペア": ["string"],
    "時間": ["iso8601-jp"],
    "方向": ["string"],
    "約定価格": ["big-decimal-positive"],
    "約定金額": ["big-decimal-positive"],
    "合計": ["big-decimal-positive"],
    "手数料": ["big-decimal"],
    "役割": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "方向"
            },
            "pattern": {
              "type": "in",
              "values": ["BUY", "SELL"]
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot-trade_",
