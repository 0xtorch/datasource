              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時間"
            },
            "transfers": [
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "BUY"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "約定金額"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "ペア"
                    },
                    "splitter": "_",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "SELL"
                  }
                ],
                "direction": "out",
