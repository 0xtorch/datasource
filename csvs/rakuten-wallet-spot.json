              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引年月日"
            },
            "transfers": [
              {
                "target": [
                  {
                    "column": "取引種別",
                    "pattern": {
                      "type": "in",
                      "values": ["買い", "売り"]
                    }
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "増加数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "増加通貨名"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "取引種別",
                    "pattern": {
                      "type": "in",
                      "values": ["買い", "売り"]
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "減少数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "減少通貨名"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "手数料数量",
                    "pattern": {
                      "type": "positive"
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
