    "取引年月日": ["datetime-jp"],
    "取引種別": ["string"],
    "取引形態": ["blank", "string"],
    "通貨ペア": ["string"],
    "増加通貨名": ["blank", "string"],
    "増加数量": ["blank", "big-decimal"],
    "減少通貨名": ["blank", "string"],
