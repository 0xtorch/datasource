          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": {
              "type": "in",
              "values": ["買い", "売り"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "増加通貨名"
            },
            "pattern": {
              "type": "defined"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "増加数量"
            },
            "pattern": {
              "type": "defined"
            }
