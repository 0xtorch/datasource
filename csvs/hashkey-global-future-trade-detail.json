{
  "id": "hashkey-global-future-trade-detail",
  "service": "hashkey-global",
  "formatter": {
    "Time（UTC+0）": ["iso8601"],
    "Futures": ["string"],
    "Order number": ["string"],
    "Trade ID": ["string"],
    "Side": ["string"],
    "Price": ["string"],
    "Filled": ["string"],
    "Service Fee": ["string"],
    "Achieve profit and loss": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "realize profit",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Achieve profit and loss"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "^\\d+\\.\\d+ [a-zA-Z0-9]+$"
            }
          }
        ],
        "generators": [
          {
            "type": "realize-profit",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-trade_",
                { "type": "cell", "column": "Trade ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time（UTC+0）"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "Futures" },
                ": ",
                { "type": "cell", "column": "Side" },
                " / ",
                { "type": "cell", "column": "Filled" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Achieve profit and loss"
                    },
                    "splitter": " ",
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Achieve profit and loss"
                    },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "realize loss",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Achieve profit and loss"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "^-\\d+(\\.\\d+)? [a-zA-Z0-9]+$"
            }
          }
        ],
        "generators": [
          {
            "type": "realize-loss",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-trade_",
                { "type": "cell", "column": "Trade ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time（UTC+0）"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "Futures" },
                ": ",
                { "type": "cell", "column": "Side" },
                " / ",
                { "type": "cell", "column": "Filled" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Achieve profit and loss"
                    },
                    "splitter": " ",
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Achieve profit and loss"
                    },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "fee",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Service Fee"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "^\\d+\\.\\d+ [a-zA-Z0-9]+$"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-trade_",
                { "type": "cell", "column": "Trade ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time（UTC+0）"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "Futures" },
                ": ",
                { "type": "cell", "column": "Side" },
                " / ",
                { "type": "cell", "column": "Filled" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Service Fee"
                    },
                    "splitter": " ",
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Service Fee"
                    },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "ignore",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Service Fee"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "^0 [a-zA-Z0-9]+$"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "Achieve profit and loss"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "^0 [a-zA-Z0-9]+$"
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-trade_",
                { "type": "cell", "column": "Trade ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time（UTC+0）"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "Futures" },
                ": ",
                { "type": "cell", "column": "Side" },
                " / ",
                { "type": "cell", "column": "Filled" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  }
}
