{
  "id": "binance-spot-trade",
  "service": "binance",
  "formatter": {
    "Date(UTC)": ["iso8601"],
    "Pair": ["string"],
    "Side": ["string"],
    "Price": ["string"],
    "Executed": ["string"],
    "Amount": ["string"],
    "Fee": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "trade",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Side"
            },
            "pattern": {
              "type": "in",
              "values": ["BUY", "SELL"]
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot-trade_",
                {
                  "type": "cell",
                  "column": "Date(UTC)"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date(UTC)"
            },
            "transfers": [
              {
                "target": [
                  {
                    "column": "Side",
                    "pattern": "BUY"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Executed"
                    },
                    "splitter": {
                      "type": "reg-exp",
                      "pattern": "[A-Z]+$"
                    },
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "match",
                    "value": {
                      "type": "cell",
                      "column": "Executed"
                    },
                    "pattern": "[A-Z]+$",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Side",
                    "pattern": "SELL"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Executed"
                    },
                    "splitter": {
                      "type": "reg-exp",
                      "pattern": "[A-Z]+$"
                    },
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "match",
                    "value": {
                      "type": "cell",
                      "column": "Executed"
                    },
                    "pattern": "[A-Z]+$",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Side",
                    "pattern": "BUY"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Amount"
                    },
                    "splitter": {
                      "type": "reg-exp",
                      "pattern": "[A-Z]+$"
                    },
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "match",
                    "value": {
                      "type": "cell",
                      "column": "Amount"
                    },
                    "pattern": "[A-Z]+$",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Side",
                    "pattern": "SELL"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Amount"
                    },
                    "splitter": {
                      "type": "reg-exp",
                      "pattern": "[A-Z]+$"
                    },
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "match",
                    "value": {
                      "type": "cell",
                      "column": "Amount"
                    },
                    "pattern": "[A-Z]+$",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Fee",
                    "pattern": {
                      "type": "reg-exp",
                      "pattern": "^\\d+(\\.\\d+)?[A-Z]+$"
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Fee"
                    },
                    "splitter": {
                      "type": "reg-exp",
                      "pattern": "[A-Z]+$"
                    },
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "match",
                    "value": {
                      "type": "cell",
                      "column": "Fee"
                    },
                    "pattern": "[A-Z]+$",
                    "index": 0
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
