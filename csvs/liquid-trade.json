{
  "id": "liquid-trade",
  "service": "liquid",
  "headerStartColumns": ["Quoted currency"],
  "headerCheckRowSize": 10,
  "footerStartColumns": ["TRADE CLOSINGS"],
  "formatter": {
    "Quoted currency": ["string"],
    "Base currency": ["string"],
    "Execution": ["string"],
    "Type": ["string"],
    "Date": ["datetime-jp"],
    "Open qty": ["big-decimal"],
    "Fee": ["big-decimal"],
    "Fee currency": ["string"],
    "Amount": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "Bought / Sold",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Type"
            },
            "pattern": {
              "type": "in",
              "values": ["Bought", "Sold"]
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                {
                  "type": "cell",
                  "column": "Execution"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date"
            },
            "transfers": [
              {
                "target": [
                  {
                    "column": "Type",
                    "pattern": "Bought"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Open qty"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Base currency"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Type",
                    "pattern": "Sold"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Open qty"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Base currency"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Type",
                    "pattern": "Bought"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Quoted currency"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Type",
                    "pattern": "Sold"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Quoted currency"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Fee",
                    "pattern": {
                      "type": "positive"
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Fee currency"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "Fee",
                    "pattern": {
                      "type": "negative"
                    }
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Fee currency"
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
