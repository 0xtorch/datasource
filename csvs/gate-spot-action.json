{
  "id": "gate-spot-action",
  "service": "gate",
  "formatter": {
    "action_data": ["string"],
    "action_desc": ["string"],
    "change_amount": ["string"],
    "time": ["iso8601-jp"]
  },
  "parser": {
    "key": ["action_data"],
    "parsers": [
      {
        "description": "招待報酬 -> gift",
        "conditions": [
          {
            "value": { "type": "cell", "column": "action_desc" },
            "pattern": "招待報酬"
          }
        ],
        "generators": [
          {
            "type": "gift",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot-action_",
                { "type": "cell", "column": "time" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "time"
            },
            "comment": {
              "type": "join",
              "values": [
                "action_desc: ",
                { "type": "cell", "column": "action_desc" },
                " / change_amount: ",
                { "type": "cell", "column": "change_amount" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "change_amount"
                    },
                    "splitter": " ",
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "change_amount"
                    },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      },
