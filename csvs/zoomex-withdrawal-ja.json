{
  "id": "zoomex-withdrawal-ja",
  "service": "zoomex",
  "headerStartColumns": [
    "通貨",
    "チェーンタイプ",
    "数量",
    "トランザクション手数料"
  ],
  "formatter": {
    "通貨": ["string"],
    "チェーンタイプ": ["string"],
    "数量": ["big-decimal"],
    "トランザクション手数料": ["big-decimal"],
    "ウォレットアドレス": ["string"],
    "出金メモ": ["string"],
    "状態": ["string"],
    "時刻": ["datetime-jp"]
  },
  "parser": {
    "parsers": [
      {
        "description": "fee | 状態=送金完了 & トランザクション手数料 > 0",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "状態"
            },
            "pattern": "送金完了"
          },
          {
            "value": {
              "type": "cell",
              "column": "トランザクション手数料"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "時刻"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時刻"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "通貨" },
                " to ",
                { "type": "cell", "column": "ウォレットアドレス" },
                " on ",
                { "type": "cell", "column": "チェーンタイプ" },
                " チェーン / 出金メモ: ",
                { "type": "cell", "column": "出金メモ" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "トランザクション手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "通貨"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "ignore | 状態=送金完了 & トランザクション手数料 = 0",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "状態"
            },
            "pattern": "送金完了"
          },
          {
            "value": {
              "type": "cell",
              "column": "トランザクション手数料"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "時刻"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時刻"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "通貨" },
                " to ",
                { "type": "cell", "column": "ウォレットアドレス" },
                " on ",
                { "type": "cell", "column": "チェーンタイプ" },
                " チェーン / 出金メモ: ",
                { "type": "cell", "column": "出金メモ" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "ignore | 状態!=送金完了",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "状態"
            },
            "pattern": {
              "type": "not-in",
              "values": ["送金完了"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "時刻"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時刻"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "通貨" },
                " to ",
                { "type": "cell", "column": "ウォレットアドレス" },
                " on ",
                { "type": "cell", "column": "チェーンタイプ" },
                " チェーン / 出金メモ: ",
                { "type": "cell", "column": "出金メモ" },
                " / 状態: ",
                { "type": "cell", "column": "状態" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  }
}
