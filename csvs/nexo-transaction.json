{
  "id": "next-transaction",
  "service": "nexo",
  "formatter": {
    "Transaction": ["string"],
    "Type": ["string"],
    "Input Currency": ["string"],
    "Input Amount": ["big-decimal"],
    "Output Currency": ["string"],
    "Output Amount": ["big-decimal"],
    "Fee": ["string", "big-decimal"],
    "Fee Currency": ["string"],
    "Details": ["string"],
    "Date / Time (UTC)": ["iso8601"]
  },
  "parser": {
    "parsers": [
      {
        "description": "Interest / Fixed Term Interest",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Type"
            },
            "pattern": {
              "type": "in",
              "values": ["Interest", "Fixed Term Interest"]
            }
          }
        ],
        "generators": [
          {
            "type": "staking-reward",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "Date / Time (UTC)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date / Time (UTC)"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "Type" },
                ": ",
                { "type": "cell", "column": "Output Amount" },
                { "type": "cell", "column": "Output Currency" },
                " / ",
                { "type": "cell", "column": "Details" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Output Amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Output Currency"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Top up Crypto / Withdrawal / Locking Term Deposit / Unlocking Term Deposit / Transfer In / Transfer Out / Transfer To Pro Wallet",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Type"
            },
            "pattern": {
              "type": "in",
              "values": [
                "Top up Crypto",
                "Withdrawal",
                "Locking Term Deposit",
                "Unlocking Term Deposit",
                "Transfer In",
                "Transfer Out",
                "Transfer To Pro Wallet"
              ]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "Date / Time (UTC)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date / Time (UTC)"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "Type" },
                ": ",
                { "type": "cell", "column": "Output Amount" },
                { "type": "cell", "column": "Output Currency" },
                " / ",
                { "type": "cell", "column": "Details" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "Exchange / Exchange Deposited On / Exchange Collateral",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Type"
            },
            "pattern": {
              "type": "in",
              "values": [
                "Exchange",
                "Exchange Deposited On",
                "Exchange Collateral"
              ]
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "Date / Time (UTC)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date / Time (UTC)"
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Output Amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Output Currency"
                  }
                }
              },
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Input Amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Input Currency"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Deposit To Exchange",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Type"
            },
            "pattern": "Deposit To Exchange"
          },
          {
            "value": {
              "type": "cell",
              "column": "Fee"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "Date / Time (UTC)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date / Time (UTC)"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "Type" },
                ": ",
                { "type": "cell", "column": "Output Amount" },
                { "type": "cell", "column": "Output Currency" },
                " / Fee: ",
                { "type": "cell", "column": "Fee" },
                { "type": "cell", "column": "Fee Currency" },
                " / ",
                { "type": "cell", "column": "Details" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Fee Currency"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Exchange Cashback",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Type"
            },
            "pattern": "Exchange Cashback"
          }
        ],
        "generators": [
          {
            "type": "gift",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "Date / Time (UTC)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date / Time (UTC)"
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Output Amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Output Currency"
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
