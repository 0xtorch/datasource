      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": "送付"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料通貨"
            },
            "pattern": {
              "type": "defined"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料数量"
            },
            "pattern": {
              "type": "defined"
            }
          }
