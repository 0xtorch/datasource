                "_",
                { "type": "cell", "column": "取引日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引日時"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "取引種別" },
