{
  "id": "coincheck-standard",
  "service": "coincheck",
  "formatter": {
    "取引日時": ["datetime-jp"],
    "取引種別": ["string"],
    "増加通貨名": ["blank", "string"],
    "増加数量": ["blank", "big-decimal-positive"],
    "減少通貨名": ["blank", "string"],
    "減少数量": ["blank", "big-decimal-positive"],
    "手数料通貨": ["blank", "string"],
    "手数料数量": ["blank", "big-decimal-positive"],
    "送付先アドレス": ["string"],
    "備考": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": {
              "type": "in",
              "values": ["購入", "売却", "交換"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "増加通貨名"
            },
            "pattern": {
              "type": "defined"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "増加数量"
            },
            "pattern": {
              "type": "defined"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "減少通貨名"
            },
            "pattern": {
              "type": "defined"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "減少数量"
            },
            "pattern": {
              "type": "defined"
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "取引日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引日時"
            },
            "comment": {
              "type": "cell",
              "column": "備考"
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "増加数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "増加通貨名"
                  }
                }
              },
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "減少数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "減少通貨名"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": {
              "type": "in",
