{
  "id": "hashhub-lending-history",
  "service": "hashhub-lending",
  "formatter": {
    "取引日時": ["datetime-jp"],
    "取引種別": ["string"],
    "取引形態": ["string"],
    "通貨ペア": ["string"],
    "増加通貨名": ["string"],
    "増加数量": ["blank", "big-decimal"],
    "減少通貨名": ["string"],
    "減少数量": ["blank", "big-decimal"],
    "約定代金": ["blank", "big-decimal"],
    "約定価格": ["blank", "big-decimal"],
    "手数料通貨": ["string"],
    "手数料数量": ["blank", "big-decimal"],
    "備考": ["string"],
    "送付元アドレス": ["string"],
    "送付先アドレス": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "受取貸借料",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": "受取貸借料"
          },
          {
            "value": {
              "type": "cell",
              "column": "増加数量"
            },
            "pattern": {
              "type": "defined"
            }
          }
        ],
        "generators": [
          {
            "type": "interest",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "取引日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "取引種別: ",
                { "type": "cell", "column": "取引種別" },
                " / 取引形態: ",
                { "type": "cell", "column": "取引形態" },
                " / 備考: ",
                { "type": "cell", "column": "備考" },
                " / 増加: ",
                { "type": "cell", "column": "増加数量" },
                " ",
                { "type": "cell", "column": "増加通貨名" },
                " / 減少: ",
                { "type": "cell", "column": "減少数量" },
                " ",
                { "type": "cell", "column": "減少通貨名" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "増加数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "増加通貨名"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "返済 (手数料あり)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": "返済"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料数量"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "取引日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "取引種別: ",
                { "type": "cell", "column": "取引種別" },
                " / 取引形態: ",
                { "type": "cell", "column": "取引形態" },
                " / 備考: ",
                { "type": "cell", "column": "備考" },
                " / 増加: ",
                { "type": "cell", "column": "増加数量" },
                " ",
                { "type": "cell", "column": "増加通貨名" },
                " / 減少: ",
                { "type": "cell", "column": "減少数量" },
                " ",
                { "type": "cell", "column": "減少通貨名" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "手数料通貨"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "返済 (手数料なし)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": "返済"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料数量"
            },
            "pattern": {
              "type": "in",
              "values": ["", "\"0\""]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "取引日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "取引種別: ",
                { "type": "cell", "column": "取引種別" },
                " / 取引形態: ",
                { "type": "cell", "column": "取引形態" },
                " / 備考: ",
                { "type": "cell", "column": "備考" },
                " / 増加: ",
                { "type": "cell", "column": "増加数量" },
                " ",
                { "type": "cell", "column": "増加通貨名" },
                " / 減少: ",
                { "type": "cell", "column": "減少数量" },
                " ",
                { "type": "cell", "column": "減少通貨名" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "その他",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引種別"
            },
            "pattern": ""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_",
                { "type": "cell", "column": "取引日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "取引日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "取引種別: ",
                { "type": "cell", "column": "取引種別" },
                " / 取引形態: ",
                { "type": "cell", "column": "取引形態" },
                " / 備考: ",
                { "type": "cell", "column": "備考" },
                " / 増加: ",
                { "type": "cell", "column": "増加数量" },
                " ",
                { "type": "cell", "column": "増加通貨名" },
                " / 減少: ",
                { "type": "cell", "column": "減少数量" },
                " ",
                { "type": "cell", "column": "減少通貨名" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  }
}
