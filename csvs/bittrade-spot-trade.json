{
  "id": "bittrade-spot-trade",
  "service": "bittrade",
  "formatter": {
    "処理時間": ["iso8601-jp"],
    "注文ステータス": ["string"],
    "通貨ペア": ["string"],
    "方向": ["string"],
    "約定量": ["big-decimal-positive"],
    "約定価格": ["big-decimal-positive"],
    "決済通貨": ["string"],
    "約定総額": ["big-decimal-positive"],
    "基軸通貨": ["string"],
    "手数料": ["big-decimal"],
    "通貨単位": ["string"],
    "注文ID": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "方向"
            },
            "pattern": {
              "type": "in",
              "values": ["買い", "売り"]
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot-trade_",
                {
                  "type": "cell",
                  "column": "処理時間"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "処理時間"
            },
            "transfers": [
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "買い"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "約定量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "/",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "買い"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "約定総額"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "/",
                    "index": 1
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "売り"
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "約定量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "/",
                    "index": 0
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "方向",
                    "pattern": "売り"
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "約定総額"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "通貨ペア"
                    },
                    "splitter": "/",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "BTC": "crypto/bitcoin",
    "ETH": "crypto/ethereum",
    "JOC": "crypto/japan-open-chain",
    "ADA": "crypto/cardano",
    "XRP": "crypto/ripple",
    "SOL": "crypto/solana",
    "SHIB": "crypto/shiba-inu",
    "DOGE": "crypto/dogecoin",
    "TON": "crypto/the-open-network",
    "APT": "crypto/aptos",
    "SXP": "crypto/swipe",
    "BCH": "crypto/bitcoin-cash",
    "SUI": "crypto/sui",
    "PLT": "crypto/palette",
    "BSV": "crypto/bitcoin-cash-sv",
    "LTC": "crypto/litecoin",
    "BNB": "crypto/binancecoin",
    "DOT": "crypto/polkadot",
    "ASTR": "crypto/astar",
    "MATIC": "crypto/polygon-ecosystem-token",
    "DAI": "crypto/dai",
    "MKR": "crypto/maker",
    "AXS": "crypto/axie-infinity",
    "SAND": "crypto/the-sandbox",
    "IOST": "crypto/iostoken",
    "XLM": "crypto/stellar",
    "DEP": "crypto/deapcoin",
    "XEM": "crypto/nem",
    "BOBA": "crypto/boba-network",
    "FLR": "crypto/flare-networks",
    "ATOM": "crypto/cosmos",
    "XYM": "crypto/symbol",
    "ETC": "crypto/ethereum-classic",
    "EOS": "crypto/eos",
    "TRX": "crypto/tron",
    "JASMY": "crypto/jasmycoin",
    "XTZ": "crypto/tezos",
    "COT": "crypto/cosplay-token-2",
    "QTUM": "crypto/qtum",
    "BAT": "crypto/basic-attention-token",
    "ONT": "crrypto/ontology",
    "MONA": "crypto/monacoin"
  }
}
