{
  "id": "hashkey-global-withdrawal",
  "service": "hashkey-global",
  "sheetName": "Sheet1",
  "headerStartColumns": ["Time"],
  "headerCheckRowSize": 10,
  "formatter": {
    "Time": ["iso8601"],
    "Order ID": ["string"],
    "Asset": ["string"],
    "Network": ["string"],
    "Amount": ["string"],
    "Address": ["string"],
    "Status": ["string"],
    "Txn Hash": ["string"],
    "Fees": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Status"
            },
            "pattern": "Succeeded"
          },
          {
            "value": {
              "type": "cell",
              "column": "Fees"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "^\\d+(\\.\\d+)?[a-zA-Z]+$"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "Order ID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw ",
                { "type": "cell", "column": "Amount" },
                { "type": "cell", "column": "Asset" },
                " to ",
                { "type": "cell", "column": "Address" },
                " on ",
                { "type": "cell", "column": "Network" },
                " Network | Txn: ",
                { "type": "cell", "column": "Txn Hash" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "Fees"
                    },
                    "splitter": {
                      "type": "reg-exp",
                      "pattern": "[a-zA-Z]+$"
                    },
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "match",
                    "value": {
                      "type": "cell",
                      "column": "Fees"
                    },
                    "pattern": "[a-zA-Z]+$",
                    "index": 0
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
