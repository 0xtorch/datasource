{
  "id": "gate-withdrawal",
  "service": "gate",
  "formatter": {
    "Order ID": ["string"],
    "Time": ["iso8601-jp"],
    "Network": ["string"],
    "Address": ["string"],
    "TxID": ["string"],
    "Coin": ["string"],
    "Trading Fee": ["big-decimal"],
    "Status": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "Withdrawal fee (Trading Fee > 0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Status"
            },
            "pattern": "完了しました"
          },
          {
            "value": {
              "type": "cell",
              "column": "Trading Fee"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                { "type": "cell", "column": "Order ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "Address"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "Network"
                },
                " Network | Tx: ",
                {
                  "type": "cell",
                  "column": "TxID"
                }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Trading Fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Coin"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "Withdrawal fee (Trading Fee = 0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Status"
            },
            "pattern": "完了しました"
          },
          {
            "value": {
              "type": "cell",
              "column": "Trading Fee"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                { "type": "cell", "column": "Order ID" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "Address"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "Network"
                },
                " Network | Tx: ",
                {
                  "type": "cell",
                  "column": "TxID"
                }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "ASM": "crypto/assemble-protocol",
    "ASMATCH": "crypto/asmatch",
    "COM": "crypto/com-ordinals",
    "EVADORE": "crypto/evadore",
    "EVADORE_OLD": "crypto/evadore",
    "MEME": "crypto/memecoin-2",
    "MEMETOON": "crypto/memetoon",
    "PIGCOIN": "crypto/pigcoin-2",
    "SMT": "crypto/smartmesh",
    "SMTX": "crypto/swarm-markets",
    "STAR": "crypto/filestar",
    "STARHEROES": "crypto/starheroes",
    "UP": "crypto/up",
    "USDTEST": "crypto/0xtorch-gate-usdtest"
  }
}
