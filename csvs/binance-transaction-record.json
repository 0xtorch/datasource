{
  "id": "binance-transaction-record",
  "service": "binance",
  "formatter": {
    "UTC_Time": ["iso8601"],
    "Account": ["string"],
    "Operation": ["string"],
    "Coin": ["string"],
    "Change": ["string"],
    "Remark": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "interest | Simple Earn Flexible Interest (Change > 0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Operation"
            },
            "pattern": {
              "type": "in",
              "values": ["Simple Earn Flexible Interest"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "Change"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "interest",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_transaction_",
                {
                  "type": "cell",
                  "column": "UTC_Time"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "UTC_Time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Operation: ",
                { "type": "cell", "column": "Operation" },
                " / Change: ",
                { "type": "cell", "column": "Change" },
                { "type": "cell", "column": "Coin" },
                " / Remark: ",
                { "type": "cell", "column": "Remark" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Change"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Coin"
                  }
