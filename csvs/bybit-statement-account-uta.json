        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Type"
            },
            "pattern": "TOKENS_REDEMPTION"
          },
          {
            "value": {
              "type": "cell",
              "column": "Cash Flow"
            },
            "pattern": { "type": "positive" }
