              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Cash Flow"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
