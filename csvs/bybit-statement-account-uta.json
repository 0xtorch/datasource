              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time(UTC)"
            },
            "comment": {
              "type": "join",
              "values": [
                "Type: ",
                { "type": "cell", "column": "Type" },
                " / ",
                { "type": "cell", "column": "Cash Flow" },
