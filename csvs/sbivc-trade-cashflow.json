          {
            "value": {
              "type": "cell",
              "column": "入出金の種別"
            },
            "pattern": "入金・入庫（調整）"
          }
        ],
        "generators": [
          {
            "type": "income",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_cashflow_",
                { "type": "cell", "column": "日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "入出金の種別: ",
                { "type": "cell", "column": "入出金の種別" },
                " / 数量: ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "暗号資産/通貨" },
                " / 備考: ",
                { "type": "cell", "column": "備考" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "数量"
                  }
