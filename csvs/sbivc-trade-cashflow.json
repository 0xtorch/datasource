{
  "id": "sbivc-trade-cashflow",
  "service": "sbivc-trade",
  "formatter": {
    "日時": ["datetime-jp"],
    "入出金の種別": ["string"],
    "暗号資産/通貨": ["string"],
    "数量": ["big-decimal"],
    "備考": ["string"]
