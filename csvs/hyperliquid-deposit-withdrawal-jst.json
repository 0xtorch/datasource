{
  "id": "hyperliquid-deposit-withdrawal-jst",
  "service": "hyperliquid",
  "formatter": {
    "time": ["datetime-jp"],
    "action": ["string"],
    "accountValueChange": ["string"],
    "fee": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "airdrop | .*distribution$",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "action"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": ".*distribution$"
            }
          }
        ],
        "generators": [
          {
            "type": "airdrop",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "time" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Action: ",
                { "type": "cell", "column": "action" },
                " / Change: ",
                { "type": "cell", "column": "accountValueChange" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "accountValueChange"
                    },
                    "splitter": " ",
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "accountValueChange"
                    },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "fee | withdrawal (fee>0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "action"
            },
            "pattern": "withdrawal"
          },
          {
            "value": {
              "type": "cell",
              "column": "fee"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "time" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Action: ",
                { "type": "cell", "column": "action" },
                " / Change: ",
                { "type": "cell", "column": "accountValueChange" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "accountValueChange"
                    },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "ignore | deposit / withdrawal (fee=0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "action"
            },
            "pattern": {
              "type": "in",
              "values": ["deposit", "withdrawal"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "fee"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "time" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Action: ",
                { "type": "cell", "column": "action" },
                " / Change: ",
                { "type": "cell", "column": "accountValueChange" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "ignore | .*transfer$ (fee=0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "action"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": ".*transfer$"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "fee"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_deposit-withdrawal_",
                { "type": "cell", "column": "time" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "time"
            },
            "comment": {
              "type": "join",
              "values": [
                "Action: ",
                { "type": "cell", "column": "action" },
                " / Change: ",
                { "type": "cell", "column": "accountValueChange" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "PURR": "crypto/purr-2"
  }
}
