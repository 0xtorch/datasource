{
  "id": "binance-withdrawal-ja",
  "service": "binance",
  "formatter": {
    "日付（UTC+0）": ["iso8601"],
    "暗号資産": ["string"],
    "ネットワーク": ["string"],
    "数量": ["big-decimal-positive"],
    "手数料": ["big-decimal"],
    "住所": ["string"],
    "トランザクションID（TXID）": ["string"],
    "ステータス": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "ステータス"
            },
            "pattern": "Completed"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "ネットワーク"
                },
                "_",
                {
                  "type": "cell",
                  "column": "トランザクションID（TXID）"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日付（UTC+0）"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "住所"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "ネットワーク"
                },
                " ネットワーク | TXID: ",
                {
                  "type": "cell",
                  "column": "トランザクションID（TXID）"
                }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "暗号資産"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "ステータス"
            },
            "pattern": "Completed"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdraw_",
                {
                  "type": "cell",
                  "column": "ネットワーク"
                },
                "_",
                {
                  "type": "cell",
                  "column": "トランザクションID（TXID）"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日付（UTC+0）"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "住所"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "ネットワーク"
                },
                " ネットワーク | TXID: ",
                {
                  "type": "cell",
                  "column": "トランザクションID（TXID）"
                }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "ステータス"
            },
            "pattern": {
              "type": "not-in",
              "values": ["Completed"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdraw_",
                {
                  "type": "cell",
                  "column": "ネットワーク"
                },
                "_",
                {
                  "type": "cell",
                  "column": "トランザクションID（TXID）"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日付（UTC+0）"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "住所"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "ネットワーク"
                },
                " ネットワーク | TXID: ",
                {
                  "type": "cell",
                  "column": "トランザクションID（TXID）"
                }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "COCOS": "crypto/cocos-bcx",
    "KAIA": "crypto/kaia",
    "KLAY": "crypto/klaytn",
    "RNDR": "crypto/render-token",
    "TOMO": "crypto/tomochain"
  }
}
