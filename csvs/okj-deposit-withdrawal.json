{
  "id": "okj-deposit-withdrawal",
  "service": "okj",
  "formatter": {
    "入出金ID": ["string"],
    "日時": ["datetime-jp"],
    "資産": ["string"],
    "入出金内容": ["string"],
    "数量": ["big-decimal"],
    "手数料": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "ステーキング報酬 > staking-reward (手数料=0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金内容"
            },
            "pattern": "ステーキング報酬"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "staking-reward",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "入出金ID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                {
                  "type": "cell",
                  "column": "入出金内容"
                },
                ": ",
                {
                  "type": "cell",
                  "column": "数量"
                },
                {
                  "type": "cell",
                  "column": "資産"
                }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "資産"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "キャンペーン > gift (手数料=0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金内容"
            },
            "pattern": "キャンペーン"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "gift",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "入出金ID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                {
                  "type": "cell",
                  "column": "入出金内容"
                },
                ": ",
                {
                  "type": "cell",
                  "column": "数量"
                },
                {
                  "type": "cell",
                  "column": "資産"
                }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "資産"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "出金・出庫 > fee (手数料<0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金内容"
            },
            "pattern": "出金・出庫"
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料"
            },
            "pattern": {
              "type": "negative"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "入出金ID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                {
                  "type": "cell",
                  "column": "入出金内容"
                },
                ": ",
                {
                  "type": "cell",
                  "column": "数量"
                },
                {
                  "type": "cell",
                  "column": "資産"
                }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "資産"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "出金・出庫 / 入金・入庫 / 売買 > ignore (手数料=0)",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金内容"
            },
            "pattern": {
              "type": "in",
              "values": ["出金・出庫", "入金・入庫", "売買"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "手数料"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                {
                  "type": "cell",
                  "column": "入出金ID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                {
                  "type": "cell",
                  "column": "入出金内容"
                },
                ": ",
                {
                  "type": "cell",
                  "column": "数量"
                },
                {
                  "type": "cell",
                  "column": "資産"
                }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
