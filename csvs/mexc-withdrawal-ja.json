{
  "id": "mexc-withdrawal-ja",
  "service": "mexc",
  "formatter": {
    "ステータス": ["string"],
    "時間": ["iso8601-jp"],
    "暗号資産": ["string"],
    "ネットワーク": ["string"],
    "出金先アドレス": ["string"],
    "トラザクションID (TXID)": ["string"],
    "取引手数料": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "withdrawal fee",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "ステータス"
            },
            "pattern": "出金成功"
          },
          {
            "value": {
              "type": "cell",
              "column": "取引手数料"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_",
                { "type": "cell", "column": "時間" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時間"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "出金先アドレス"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "ネットワーク"
                },
                " Network | Tx: ",
                {
                  "type": "cell",
                  "column": "トラザクションID (TXID)"
                }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "取引手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "暗号資産"
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "ALT": "crypto/aptos-launch-token",
    "ALTLAYER": "crypto/altlayer",
    "ASM": "crypto/as-monaco-fan-token",
    "ASMATCH": "crypto/asmatch",
    "COM": "crypto/com-ordinals",
    "EVADORE": "crypto/evadore",
    "MEME": "crypto/memecoin-2",
    "MEMETOON": "crypto/memetoon",
    "PIGCOIN": "crypto/pigcoin-2"
  }
}
