{
  "id": "mexc-future-flow-ja-jst",
  "service": "mexc",
  "formatter": {
    "時間(UTC+09:00)": ["iso8601-jp"],
    "先物取引ペア": ["string"],
    "暗号資産": ["string"],
    "資金の種類": ["string"],
    "資金フローの種類": ["string"],
    "金額": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "FUNDING > profit",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "資金の種類"
            },
            "pattern": "FUNDING"
          },
          {
            "value": {
              "type": "cell",
              "column": "金額"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "derivative-profit",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-flow_",
                { "type": "cell", "column": "時間(UTC+09:00)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時間(UTC+09:00)"
            },
            "comment": {
              "type": "join",
              "values": [
                "資金の種類: ",
                { "type": "cell", "column": "資金の種類" },
                " / 先物取引ペア: ",
                { "type": "cell", "column": "先物取引ペア" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "金額"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "暗号資産"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "FUNDING > fee",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "資金の種類"
            },
            "pattern": "FUNDING"
          },
          {
            "value": {
              "type": "cell",
              "column": "金額"
            },
            "pattern": {
              "type": "negative"
            }
          }
        ],
        "generators": [
          {
            "type": "derivative-fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-flow_",
                { "type": "cell", "column": "時間(UTC+09:00)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時間(UTC+09:00)"
            },
            "comment": {
              "type": "join",
              "values": [
                "資金の種類: ",
                { "type": "cell", "column": "資金の種類" },
                " / 先物取引ペア: ",
                { "type": "cell", "column": "先物取引ペア" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "金額"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "暗号資産"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "ignore",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "資金の種類"
            },
            "pattern": {
              "type": "in",
              "values": ["CLOSE_POSITION", "FEE", "LIQUIDATION", "TRANSFER"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-flow_",
                { "type": "cell", "column": "時間(UTC+09:00)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "時間(UTC+09:00)"
            },
            "comment": {
              "type": "join",
              "values": [
                "資金の種類: ",
                { "type": "cell", "column": "資金の種類" },
                " / 先物取引ペア: ",
                { "type": "cell", "column": "先物取引ペア" },
                " / ",
                { "type": "cell", "column": "金額" },
                { "type": "cell", "column": "暗号資産" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "ALT": "crypto/aptos-launch-token",
    "ALTLAYER": "crypto/altlayer",
    "ASM": "crypto/as-monaco-fan-token",
    "ASMATCH": "crypto/asmatch",
    "COM": "crypto/com-ordinals",
    "EVADORE": "crypto/evadore",
    "GAMEVIRTUAL": "crypto/game-by-virtuals",
    "HOLDSTATION": "crypto/holdstation",
    "JOC": "crypto/japan-open-chain",
    "MEME": "crypto/memecoin-2",
    "MEMETOON": "crypto/memetoon",
    "MNEMO": "crypto/mnemonics",
    "NEKTAR": "crypto/nektar-network",
    "PIG": "crypto/pig-finance",
    "PIGCOIN": "crypto/pigcoin-2",
    "WATERSOL": "crypto/water-coin"
  }
}
