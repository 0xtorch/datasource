{
  "id": "dmmbitcoin-cashflow",
  "service": "dmmbitcoin",
  "formatter": {
    "日時": ["datetime-jp"],
    "口座種別": ["string"],
    "入出金の種別": ["string"],
    "": ["string"],
    "数量": ["big-decimal"],
    "評価レート": ["big-decimal"],
    "入出金額（円）": ["big-decimal"],
    "備考": ["string"]
  },
  "$comment": "空のカラムがある。",
  "parser": {
    "parsers": [
      {
        "description": "[ウォレ,トレ] x <通貨名> x [振替,<空>] x [入金,出金]",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金の種別"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "(振替)?(入金|出金)$"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "口座種別"
            },
            "pattern": {
              "type": "in",
              "values": ["ウォレット", "トレード"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_cashflow_",
                { "type": "cell", "column": "日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "口座種別: ",
                { "type": "cell", "column": "口座種別" },
                " / 入出金の種別: ",
                { "type": "cell", "column": "入出金の種別" },
                " / 数量: ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "" },
                " / 備考: ",
                { "type": "cell", "column": "備考" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "CB",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金の種別"
            },
            "pattern": "キャッシュバック"
          },
          {
            "value": {
              "type": "cell",
              "column": ""
            },
            "pattern": "JPY"
          }
        ],
        "generators": [
          {
            "type": "cash-back",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_cashflow_",
                { "type": "cell", "column": "日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "入出金の種別: ",
                { "type": "cell", "column": "入出金の種別" },
                " / 数量: ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "" },
                " / 備考: ",
                { "type": "cell", "column": "備考" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": { "type": "cell", "column": "" }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "出金手数料",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金の種別"
            },
            "pattern": {
              "type": "reg-exp",
              "pattern": "出金手数料$"
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "口座種別"
            },
            "pattern": "ウォレット"
          },
          {
            "value": {
              "type": "cell",
              "column": "数量"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "$comment": "ゼロフィーは表現できなさそう",

            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_cashflow_",
                { "type": "cell", "column": "日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "入出金の種別: ",
                { "type": "cell", "column": "入出金の種別" },
                " / 数量: ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "" },
                " / 備考: ",
                { "type": "cell", "column": "備考" }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "description": "現物取引",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "入出金の種別"
            },
            "pattern": "現物取引"
          },
          {
            "value": {
              "type": "cell",
              "column": "口座種別"
            },
            "pattern": "トレード"
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_cashflow_",
                { "type": "cell", "column": "日時" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "口座種別: ",
                { "type": "cell", "column": "口座種別" },
                " / 入出金の種別: ",
                { "type": "cell", "column": "入出金の種別" },
                " / 数量: ",
                { "type": "cell", "column": "数量" },
                { "type": "cell", "column": "" },
                " / 備考: ",
                { "type": "cell", "column": "備考" }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "BTC": "crypto/bitcoin",
    "ETH": "crypto/ethereum",
    "XRP": "crypto/ripple",
    "LTC": "crypto/litecoin",
    "BCH": "crypto/bitcoin-cash",
    "ADA": "crypto/cardano",
    "ALGO": "crypto/algorand",
    "APE": "crypto/apecoin",
