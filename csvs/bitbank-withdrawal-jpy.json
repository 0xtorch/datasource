{
  "id": "bitbank-withdrawal-jpy",
  "service": "bitbank",
  "formatter": {
    "日時": ["datetime-jp"],
    "数量": ["big-decimal"],
    "手数料": ["big-decimal"],
    "出金先口座": ["string"],
    "ステータス": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "DONE > fee",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "ステータス"
            },
            "pattern": "DONE"
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_jpy_",
                {
                  "type": "cell",
                  "column": "日時"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "「",
                {
                  "type": "cell",
                  "column": "出金先口座"
                },
                "」に出金 / ステータス: ",
                {
                  "type": "cell",
                  "column": "ステータス"
                }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "id",
                  "value": "fiat/jpy",
                  "splitter": "/"
                }
              }
            ]
          }
        ]
      },
      {
        "description": "CANCELED > ignore",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "ステータス"
            },
            "pattern": "CANCELED"
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdrawal_jpy_",
                {
                  "type": "cell",
                  "column": "日時"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "comment": {
              "type": "join",
              "values": [
                "「",
                {
                  "type": "cell",
                  "column": "出金先口座"
                },
                "」に出金 / ステータス: ",
                {
                  "type": "cell",
                  "column": "ステータス"
                }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  }
}
