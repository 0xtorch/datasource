{
  "id": "kucoin-funding-account-jst",
  "service": "kucoin",
  "formatter": {
    "UID": ["string"],
    "Account Type": ["string"],
    "Currency": ["string"],
    "Side": ["string"],
    "Amount": ["big-decimal"],
    "Fee": ["big-decimal"],
    "Time(UTC+09:00)": ["iso8601-jp"],
    "Remark": ["string"],
    "Type": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "Referral Rewards",
        "conditions": [
          {
            "value": { "type": "cell", "column": "Side" },
            "pattern": "Deposit"
          },
          {
            "value": { "type": "cell", "column": "Type" },
            "pattern": "Referral Rewards"
          }
        ],
        "generators": [
          {
            "type": "gift",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_funding_",
                { "type": "cell", "column": "Time(UTC+09:00)" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Time(UTC+09:00)"
            },
            "comment": {
              "type": "join",
              "values": [
                "remark: ",
                { "type": "cell", "column": "Remark" },
                ", type: ",
                { "type": "cell", "column": "Type" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Currency"
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
