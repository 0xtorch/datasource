{
  "id": "gate-future-action",
  "service": "gate",
  "formatter": {
    "Time": ["iso8601-jp"],
    "Contacts": ["string"],
    "Action Type": ["string"],
    "Change": ["string"],
    "Balance": ["string"],
    "Remarks": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "取引手数料 -> fee",
        "conditions": [
          {
            "value": { "type": "cell", "column": "Action Type" },
            "pattern": "取引手数料"
          },
          {
            "value": { "type": "cell", "column": "Change" },
            "pattern": {
              "type": "reg-exp",
              "pattern": "^-\\d+(\\.\\d+)? [a-zA-Z0-9]+$"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_future-action_",
                { "type": "cell", "column": "Time" }
              ]
            },
            "timestamp": { "type": "cell", "column": "Time" },
            "comment": {
              "type": "join",
              "values": [
                "contacts: ",
                { "type": "cell", "column": "Contacts" },
                " / Action Type: ",
                { "type": "cell", "column": "Action Type" },
                " / Change: ",
                { "type": "cell", "column": "Change" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": { "type": "cell", "column": "Change" },
                    "splitter": " ",
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": { "type": "cell", "column": "Change" },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "取引手数料 -> income",
