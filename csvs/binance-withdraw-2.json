{
  "id": "binance-withdraw-2",
  "service": "binance",
  "formatter": {
    "Date(UTC+0)": ["iso8601"],
    "Coin": ["blank", "string"],
    "Network": ["string"],
    "Amount": ["big-decimal-positive"],
    "Fee": ["big-decimal"],
    "Address": ["string"],
    "TXID": ["string"],
    "Status": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Status"
            },
            "pattern": "Completed"
          },
          {
            "value": {
              "type": "cell",
              "column": "Fee"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdraw_",
                {
                  "type": "cell",
                  "column": "Network"
                },
                "_",
                {
                  "type": "cell",
                  "column": "TXID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date(UTC+0)"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "Address"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "Network"
                },
                " Network | TXID: ",
                {
                  "type": "cell",
                  "column": "TXID"
                }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "Fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "Coin"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Status"
            },
            "pattern": "Completed"
          },
          {
            "value": {
              "type": "cell",
              "column": "Fee"
            },
            "pattern": "\"0\""
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdraw_",
                {
                  "type": "cell",
                  "column": "Network"
                },
                "_",
                {
                  "type": "cell",
                  "column": "TXID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date(UTC+0)"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "Address"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "Network"
                },
                " Network | TXID: ",
                {
                  "type": "cell",
                  "column": "TXID"
                }
              ]
            },
            "transfers": []
          }
        ]
      },
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "Status"
            },
            "pattern": {
              "type": "not-in",
              "values": ["Completed"]
            }
          }
        ],
        "generators": [
          {
            "type": "ignore",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_withdraw_",
                {
                  "type": "cell",
                  "column": "Network"
                },
                "_",
                {
                  "type": "cell",
                  "column": "TXID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "Date(UTC+0)"
            },
            "comment": {
              "type": "join",
              "values": [
                "Withdraw to ",
                {
                  "type": "cell",
                  "column": "Address"
                },
                " on ",
                {
                  "type": "cell",
                  "column": "Network"
                },
                " Network | TXID: ",
                {
                  "type": "cell",
                  "column": "TXID"
                }
              ]
            },
            "transfers": []
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "COCOS": "crypto/cocos-bcx"
  }
}
