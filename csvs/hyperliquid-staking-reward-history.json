{
  "id": "hyperliquid-staking-reward-history",
  "service": "hyperliquid",
  "formatter": {
    "time": ["unixtime-ms"],
    "source": ["string"],
    "amount": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "staking-reward | amount>0",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "amount"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
        "generators": [
          {
            "type": "staking-reward",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_staking-reward_",
                { "type": "cell", "column": "time" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "time"
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "amount"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": "HYPE"
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "HYPE": "crypto/hyperliquid"
  }
}
