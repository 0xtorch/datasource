{
  "id": "bitget-saving",
  "service": "bitget",
  "formatter": {
    "product name": ["string"],
    "amount": ["string"],
    "profit type": ["string"],
    "date": ["iso8601-jp"],
    "type": ["string"],
    "status": ["string"]
  },
  "parser": {
    "parsers": [
      {
        "description": "saving profit",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "status"
            },
            "pattern": "Completed"
          }
        ],
        "generators": [
          {
            "type": "interest",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_saving_",
                { "type": "cell", "column": "date" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "date"
            },
            "comment": {
              "type": "join",
              "values": [
                "product name: ",
                { "type": "cell", "column": "product name" },
                " / profit type: ",
                { "type": "cell", "column": "profit type" },
                " / type: ",
                { "type": "cell", "column": "type" }
              ]
            },
            "transfers": [
              {
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "amount"
                    },
                    "splitter": " ",
                    "index": 0
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "split",
                    "value": {
                      "type": "cell",
                      "column": "amount"
                    },
                    "splitter": " ",
                    "index": 1
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "BGBTC": "crypto/0xtorch-bitget-bgbtc",
    "BGSOL": "crypto/0xtorch-bitget-bgsol"
  }
}
