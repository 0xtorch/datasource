{
  "id": "okj-trade",
  "service": "okj",
  "formatter": {
    "注文ID": ["string"],
    "日時": ["datetime-jp"],
    "通貨": ["string"],
    "取引内容": ["string"],
    "約定数量": ["big-decimal"],
    "手数料": ["big-decimal"]
  },
  "parser": {
    "key": ["注文ID"],
    "parsers": [
      {
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "取引内容"
            },
            "pattern": {
              "type": "in",
              "values": ["購入", "売却"]
            }
          }
        ],
        "generators": [
          {
            "type": "trade",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_trade_",
                {
                  "type": "cell",
                  "column": "注文ID"
                }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "日時"
            },
            "transfers": [
              {
                "target": [
                  {
                    "column": "約定数量",
                    "pattern": {
                      "type": "positive"
                    }
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "約定数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "通貨"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "手数料",
                    "pattern": {
                      "type": "positive"
                    }
                  }
                ],
                "direction": "in",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "通貨"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "約定数量",
                    "pattern": {
                      "type": "negative"
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "約定数量"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "通貨"
                  }
                }
              },
              {
                "target": [
                  {
                    "column": "手数料",
                    "pattern": {
                      "type": "negative"
                    }
                  }
                ],
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "手数料"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "通貨"
                  }
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "symbolAssetMap": {
    "BTC": "crypto/bitcoin",
    "ETH": "crypto/ethereum",
    "ADA": "crypto/cardano",
    "APE": "crypto/apecoin",
    "APT": "crypto/aptos",
    "ARB": "crypto/arbitrum",
    "ASTR": "crypto/astar",
    "AVAX": "crypto/avalanche-2",
    "BAT": "crypto/basic-attention-token",
    "BCH": "crypto/bitcoin-cash",
    "DAI": "crypto/dai",
    "DEP": "crypto/deapcoin",
    "DOGE": "crypto/dogecoin",
    "DOT": "crypto/polkadot",
    "ENJ": "crypto/enjincoin",
    "ETC": "crypto/ethereum-classic",
    "FIL": "crypto/filecoin",
    "FNCT": "crypto/financie-token",
    "IOST": "crypto/iostoken",
    "IOTX": "crypto/iotex",
    "KAIA": "crypto/kaia",
    "LSK": "crypto/lisk",
    "LTC": "crypto/litecoin",
    "MASK": "crypto/mask-network",
    "MKR": "crypto/maker",
    "NEO": "crypto/neo",
    "OAS": "crypto/oasys",
    "OP": "crypto/optimism",
    "OKB": "crypto/okb",
    "PEPE": "crypto/pepe",
    "PLT": "crypto/palette",
    "POL": "crypto/polygon-ecosystem-token",
    "QTUM": "crypto/qtum",
    "SAND": "crypto/the-sandbox",
    "SHIB": "crypto/shiba-inu",
    "SOL": "crypto/solana",
    "SUI": "crypto/sui",
    "TON": "crypto/the-open-network",
    "TRX": "crypto/tron",
    "XLM": "crypto/stellar",
    "XRP": "crypto/ripple",
    "XTZ": "crypto/tezos",
    "ZIL": "crypto/zilliqa"
  }
}
