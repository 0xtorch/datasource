{
  "id": "bitget-future-trade",
  "service": "bitget",
  "formatter": {
    "order": ["string"],
    "date": ["iso8601-jp"],
    "coin": ["string"],
    "futures": ["string"],
    "type": ["string"],
    "sum": ["big-decimal"],
    "fee": ["big-decimal"]
  },
  "parser": {
    "parsers": [
      {
        "description": "open position fee",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "type"
            },
            "pattern": {
              "type": "in",
              "values": ["open_long", "open_short"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "sum"
            },
            "pattern": "\"0\""
          },
          {
            "value": {
              "type": "cell",
              "column": "fee"
            },
            "pattern": {
              "type": "negative"
            }
          }
        ],
        "generators": [
          {
            "type": "fee",
            "source": {
              "type": "join",
              "values": [
                { "type": "service" },
                "_spot-future_",
                { "type": "cell", "column": "order" }
              ]
            },
            "timestamp": {
              "type": "cell",
              "column": "date"
            },
            "comment": {
              "type": "join",
              "values": [
                { "type": "cell", "column": "type" },
                " : ",
                { "type": "cell", "column": "futures" }
              ]
            },
            "transfers": [
              {
                "direction": "out",
                "amount": {
                  "type": "text",
                  "value": {
                    "type": "cell",
                    "column": "fee"
                  }
                },
                "asset": {
                  "type": "symbol",
                  "value": {
                    "type": "cell",
                    "column": "coin"
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "description": "close position > realize-profit",
        "conditions": [
          {
            "value": {
              "type": "cell",
              "column": "type"
            },
            "pattern": {
              "type": "in",
              "values": ["close_long", "close_short"]
            }
          },
          {
            "value": {
              "type": "cell",
              "column": "sum"
            },
            "pattern": {
              "type": "positive"
            }
          }
        ],
